{
  "consumer": {
    "name": "ms.cart:with-pact@v3"
  },
  "interactions": [
    {
      "description": "A request for API customers",
      "providerStates": [
        {
          "name": "Customers are available"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/customers"
      },
      "response": {
        "body": [
          {
            "firstname": "foo",
            "lastname": "bar"
          },
          {
            "firstname": "foo",
            "lastname": "bar"
          },
          {
            "firstname": "foo",
            "lastname": "bar"
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "max": 3,
                  "min": 3
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "A request for API customer-orders",
      "providerStates": [
        {
          "name": "Customers orders are available"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/customer/2099d96b-1693-4f6f-b218-130238346855/orders"
      },
      "response": {
        "body": {
          "customer": "2099d96b-1693-4f6f-b218-130238346855",
          "orders": [
            {
              "id": "bddb4566-b025-4f1c-9784-802402e5ea9b",
              "quantity": 2
            },
            {
              "id": "bddb4566-b025-4f1c-9784-802402e5ea9b",
              "quantity": 2
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.customer": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
                }
              ]
            },
            "$.orders": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 2
                }
              ]
            },
            "$.orders[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                },
                {
                  "match": "type"
                }
              ]
            },
            "$.orders[*].quantity": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                },
                {
                  "match": "integer"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "10.4.0"
    },
    "pactRust": {
      "ffi": "0.3.19",
      "models": "1.0.3"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "ms.customers"
  }
}